{
  "name": "uncover",
  "version": "1.0.0",
  "description": "Stay up-to-date on your favourite books, TV shows, and more.",
  "author": "Joshua Comeau <joshwcomeau@gmail.com>",
  "private": true,
  "scripts": {
    "start:client:dev": "node build/dev-server.js",
    "start:server:dev": "NODE_ENV=development nodemon server/src --exec babel-node",
    "build:client": "node build/build.js",
    "build:server": "babel server/src -d server/dist",
    "build": "npm run build:client && npm run build:server",
    "deploy": "NODE_ENV=production fly deploy:production",
    "deploy:no-build": "npm run deploy -- -skip-build",
    "test:unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test:server": "mocha test/server/specs",
    "test:e2e": "node test/e2e/runner.js",
    "test": "npm run test:server && npm run test:unit && npm run test:e2e",
    "lint": "eslint --ext .js,.vue client test/unit/specs test/e2e/specs"
  },
  "dependencies": {
    "amazon-product-api": "0.4.3",
    "async": "2.1.4",
